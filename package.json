{
    "name": "@ruforms/forms",
    "version": "0.0.1",
    "description": "forms for RuForms",
    "keywords": [
        "ruforms",
        "react",
        "typescript",
        "forms"
    ],
    "author": "RuForms Team",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint src --ext .ts,.tsx --fix",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf dist",
        "prepublishOnly": "pnpm build",
        "prepare": "husky",
        "pre-commit": "lint-staged",
        "pre-push": "pnpm type-check && pnpm lint",
        "commitlint": "commitlint"
    },
    "dependencies": {
        "react": "^19.1.1",
        "react-dom": "^19.1.1"
    },
    "devDependencies": {
        "@types/react": "^19.1.10",
        "@types/react-dom": "^19.1.7",
        "tsup": "8.5.0",
        "typescript": "5.9.2",
        "eslint": "^9.0.0",
        "@eslint/js": "9.33.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint-plugin-react": "^7.37.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-config-prettier": "^9.1.0",
        "prettier": "^3.3.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.5",
        "@commitlint/cli": "^19.0.0",
        "@commitlint/config-conventional": "^19.0.0"
    },
    "peerDependencies": {
        "react": "^19.1.1",
        "react-dom": "^19.1.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ruforms/forms.git"
    },
    "bugs": {
        "url": "https://github.com/ruforms/forms/issues"
    },
    "homepage": "https://github.com/ruforms/forms#readme",
    "license": "MIT",
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "lint-staged": {
        "**/*.{ts,tsx}": [
            "prettier --write",
            "eslint --fix"
        ],
        "**/*.{js,jsx,json,md}": [
            "prettier --write"
        ]
    }
}
